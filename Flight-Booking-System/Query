CREATE TABLE FLIGHTBOOK_TBL (
    RECORDID      VARCHAR2(12 BYTE) PRIMARY KEY,
    PASSENGERNAME VARCHAR2(40 BYTE) NOT NULL,
    FLIGHTNUMBER  VARCHAR2(20 BYTE) NOT NULL,
    TRAVEL_DATE   DATE NOT NULL,
    SEATNO        VARCHAR2(10 BYTE) NOT NULL,
    TICKETNO      VARCHAR2(20 BYTE),
    REMARKS       VARCHAR2(100 BYTE)
);
CREATE SEQUENCE FLIGHTBOOK_SEQ
START WITH 10
MINVALUE 10
MAXVALUE 99
INCREMENT BY 1;
SELECT FLIGHTBOOK_SEQ.NEXTVAL FROM dual;